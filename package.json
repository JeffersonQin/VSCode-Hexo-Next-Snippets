{
	"name": "hexo-next-snippets",
	"displayName": "Hexo Next Snippets",
	"description": "Some Snippets and Command for Hexo Next Theme in markdown writing",
	"version": "1.0.3",
	"publisher": "JeffersonQin",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Snippets"
	],
	"activationEvents": [
		"onLanguage:markdown"
	],
	"main": "./extension.js",
	"contributes": {
		"snippets": [
			{
				"language": "markdown",
				"path": "./snippets/markdown.json"
			}
		]
	},
	"scripts": {
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.12.50",
		"@types/vscode": "^1.47.0",
		"eslint": "^6.6.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.3",
		"typescript": "^3.9.6",
		"vscode-test": "^1.4.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/JeffersonQin/VSCode-Hexo-Next-Snippets"
	},
	"homepage": "https://github.com/JeffersonQin/VSCode-Hexo-Next-Snippets/blob/master/README.md",
	"icon": "lib/images/next-icon.png"
}
